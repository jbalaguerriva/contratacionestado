            },
            "cbc:FundingProgram": "OPERACI\u00d3N INCLUIDA EN EL PROGRAMA OPERATIVO FONDO SOCIAL EUROPEO DE ARAGON 2014/2020",
            "cac:RequiredFinancialGuarantee": {
                "cbc:GuaranteeTypeCode": {
                    "@listURI": "http://contrataciondelestado.es/codice/cl/1.04/GuaranteeTypeCode-1.04.gc",
                    "#text": "2"
                },
                "cbc:AmountRate": "5"
