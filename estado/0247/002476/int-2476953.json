                        "cbc:EvaluationCriteriaTypeCode": {
                            "@listURI": "http://contrataciondelestado.es/codice/cl/2.0/TechnicalCapabilityTypeCode-2.0.gc",
                            "#text": "OSR-TECH"
                        },
                        "cbc:Description": "Indicaci\u00f3n del personal t\u00e9cnico responsable de la ejecuci\u00f3n del contrato con titulaci\u00f3n universitaria de Licenciado en Ingenier\u00eda en Telecomunicaciones, y experiencia profesional m\u00ednima de 10 a\u00f1os"
