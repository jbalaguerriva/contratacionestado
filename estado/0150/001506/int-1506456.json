            "cac:ContractExtension": {
                "cac:OptionValidityPeriod": {
                    "cbc:Description": "De la misma manera, previo acuerdo del \u00f3rgano de contrataci\u00f3n, en caso de realizarse pr\u00f3rrogas del contrato inicial para la realizaci\u00f3n de las auditor\u00edas correspondientes a los ejercicios 2018 y 2019, los informes definitivos correspondientes a dichos ejercicios deber\u00e1n entregarse en junio de 2019 y en junio de 2020 respectivamente, por lo que la duraci\u00f3n en su totalidad en cualquier caso siempre ser\u00e1 inferior a cuatro a\u00f1os."
                }
            }
        },
        "cac:TenderingTerms": {
            "cbc:RequiredCurriculaIndicator": "false",
            "cbc:VariantConstraintIndicator": "false",
            "cac:RequiredFinancialGuarantee": {
                "cbc:GuaranteeTypeCode": {
                    "@listURI": "http://contrataciondelestado.es/codice/cl/1.04/GuaranteeTypeCode-1.04.gc",
