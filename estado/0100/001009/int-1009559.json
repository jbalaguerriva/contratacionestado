        "cac:TenderingTerms": {
            "cbc:RequiredCurriculaIndicator": "true",
            "cbc:VariantConstraintIndicator": "false",
            "cbc:PriceRevisionFormulaDescription": "NO PROCEDE",
            "cac:TendererQualificationRequest": {
                "cac:RequiredBusinessClassificationScheme": {
                    "cbc:ID": "RequiredBusinessProfileCode",
                    "cac:ClassificationCategory": {
                        "cbc:CodeValue": "P*-1"
