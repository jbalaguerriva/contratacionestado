        "cac:TenderingTerms": {
            "cbc:RequiredCurriculaIndicator": "false",
            "cbc:VariantConstraintIndicator": "false",
            "cac:RequiredFinancialGuarantee": {
                "cbc:GuaranteeTypeCode": {
                    "@listURI": "http://contrataciondelestado.es/codice/cl/1.04/GuaranteeTypeCode-1.04.gc",
