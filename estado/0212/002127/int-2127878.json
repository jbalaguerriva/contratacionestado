            "cac:AwardingTerms": {
                "cac:AwardingCriteria": [
                    {
                        "cbc:Description": "Protocolo de admisi\u00f3n para pacientes de urgencias minimizando el tiempo de espera en base al sistema de registro de tiempo de urgencias (tiempo de entrada-tiempo de asistencia)",
                        "cbc:WeightNumeric": "3"
                    },
                    {
                        "cbc:Description": "Reducci\u00f3n de plazos para las Intervenciones Quir\u00fargicas (2 d\u00edas laborables). Por cada d\u00eda de reducci\u00f3n se obtendr\u00e1n 5 puntos hasta un m\u00e1ximo de 10 puntos.",
                        "cbc:WeightNumeric": "10"
                    },
                    {
                        "cbc:Description": "Reducci\u00f3n de plazos para las Consultas externas (2 d\u00edas laborables). Por cada d\u00eda de reducci\u00f3n se obtendr\u00e1n 5 puntos hasta un m\u00e1ximo de 10 puntos.",
                        "cbc:WeightNumeric": "10"
                    },
                    {
                        "cbc:Description": "Reducci\u00f3n de plazos para las Pruebas diagn\u00f3sticas (48 horas). Si se reduce el plazo en 24 horas obtendr\u00e1 5 puntos y si reduce el plazo dentro mismo d\u00eda de la petici\u00f3n 10 puntos.",
                        "cbc:WeightNumeric": "10"
                    },
                    {
                        "cbc:Description": "Existencia de paradas de transporte p\u00fablico (bus, tren, metro, tranv\u00eda, autob\u00fas interurbano, etc.) o paradas de taxi a una distancia inferior a 150 metros del centro concertado",
                        "cbc:WeightNumeric": "3"
                    },
                    {
                        "cbc:Description": "Canales de comunicaci\u00f3n entre MAZ y el centro, tanto para cuestiones administrativas como para cuestiones m\u00e9dicas. Se valorar\u00e1 para ello la existencia de un coordinador tanto m\u00e9dico como administrativo, especialmente si el administrativo es exclusivo para MAZ.",
                        "cbc:WeightNumeric": "5"
                    },
                    {
                        "cbc:Description": "Sistema de registro de la toma de datos inicial, en particular el registro de campos propios de gesti\u00f3n de mutuas colaboradoras con la seguridad social.",
                        "cbc:WeightNumeric": "2"
                    },
                    {
                        "cbc:Description": "Oferta Econ\u00f3mica",
                        "cbc:WeightNumeric": "40"
                    },
                    {
                        "cbc:Description": "Sistema de registro de prestaciones as\u00ed como la Historia Cl\u00ednica Electr\u00f3nica.",
                        "cbc:WeightNumeric": "1"
                    },
                    {
                        "cbc:Description": "Protocolo de admisi\u00f3n y recepci\u00f3n para pacientes quir\u00fargicos.",
                        "cbc:WeightNumeric": "3"
                    },
                    {
                        "cbc:Description": "Existencia de parking propio del centro o parking p\u00fablico a una distancia inferior a 150 metros del centro concertado",
                        "cbc:WeightNumeric": "5"
                    },
                    {
                        "cbc:Description": "Sistema de gesti\u00f3n de quejas y reclamaciones de los usuarios del servicio",
                        "cbc:WeightNumeric": "6"
                    },
                    {
                        "cbc:Description": "Disposici\u00f3n por parte del licitador de una persona de contacto a nivel m\u00e9dico 24 h, incluso festivos, todo el a\u00f1o.",
                        "cbc:WeightNumeric": "2"
