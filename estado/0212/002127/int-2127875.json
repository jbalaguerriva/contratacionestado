            "cac:AwardingTerms": {
                "cac:AwardingCriteria": [
                    {
                        "cbc:Description": "Oferta Econ\u00f3mica",
                        "cbc:WeightNumeric": "40"
                    },
                    {
                        "cbc:Description": "Reducci\u00f3n de plazos para las Pruebas diagn\u00f3sticas (48 horas): si se reduce el plazo en 24 horas obtendr\u00e1  14 puntos.",
                        "cbc:WeightNumeric": "14"
                    },
                    {
                        "cbc:Description": "Existencia de paradas de transporte p\u00fablico (bus, tren, metro, tranv\u00eda, autob\u00fas interurbano, etc.) o paradas de taxi a una distancia inferior a 150 metros del centro concertado.",
                        "cbc:WeightNumeric": "10"
                    },
                    {
                        "cbc:Description": "Se valorar\u00e1 el protocolo de admisi\u00f3n para pacientes de urgencias minimizando el tiempo de espera en base al sistema de clasificaci\u00f3n de criticidad/gravedad del paciente.",
                        "cbc:WeightNumeric": "1"
                    },
                    {
                        "cbc:Description": "Reducci\u00f3n de plazos para las Consultas externas (3 d\u00edas laborables): si reduce el plazo de tres d\u00edas a dos d\u00edas laborales obtendr\u00e1 20 puntos.",
                        "cbc:WeightNumeric": "20"
                    },
                    {
                        "cbc:Description": "Se valorar\u00e1n los canales de comunicaci\u00f3n entre MAZ y el centro, tanto para cuestiones administrativas como para cuestiones m\u00e9dicas. Se valorar\u00e1 para ello la existencia de un coordinador tanto m\u00e9dico como administrativo, especialmente si el administrativo es exclusivo para MAZ.",
                        "cbc:WeightNumeric": "9"
                    },
                    {
                        "cbc:Description": "Se valorar\u00e1 el sistema de gesti\u00f3n de quejas y reclamaciones de los usuarios",
                        "cbc:WeightNumeric": "2"
                    },
                    {
                        "cbc:Description": "Se valorar\u00e1 el sistema de registro de prestaciones e Historia Cl\u00ednica Electr\u00f3nica.",
                        "cbc:WeightNumeric": "2"
                    },
                    {
                        "cbc:Description": "Se valorar\u00e1 el sistema de registro de la toma de datos inicial, en particular la verificaci\u00f3n de datos de filiaci\u00f3n del paciente.",
                        "cbc:WeightNumeric": "2"
                    }
                ]
