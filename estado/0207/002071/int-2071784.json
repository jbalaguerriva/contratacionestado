                        "cbc:EvaluationCriteriaTypeCode": {
                            "@listURI": "http://contrataciondelestado.es/codice/cl/2.0/FinancialCapabilityTypeCode-2.0.gc",
                            "#text": "6"
                        },
                        "cbc:Description": "Patrimonio neto al cierre del \u00faltimo ejercicio econ\u00f3mico para el que est\u00e9 vencida la obligaci\u00f3n de aprobaci\u00f3n de cuentas anuales, por importe m\u00ednimo exigido de 15.000,00 euros."
                    }
