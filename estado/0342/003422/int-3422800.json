            "cac:AwardingTerms": {
                "cac:AwardingCriteria": [
                    {
                        "cbc:Description": "OFERTA ECON\u00d3MICA",
                        "cbc:WeightNumeric": "50"
                    },
                    {
                        "cbc:Description": "Que los sistemas de introducci\u00f3n de gases que est\u00e9n optimizados mediante un sistema de inertizaci\u00f3n para evitar adsorciones y contaminaciones de elementos como S o Si.",
                        "cbc:WeightNumeric": "7"
                    },
                    {
                        "cbc:Description": "Que el sistema de celda de reacci\u00f3n incorpore un sistema de aceleraci\u00f3n axial.",
                        "cbc:WeightNumeric": "7"
                    },
                    {
                        "cbc:Description": "Que el software permita la libre selecci\u00f3n de cualquier modo de reacci\u00f3n para todas las masas entre 5 y 260 sin necesidad de ajustes por parte del usuario",
                        "cbc:WeightNumeric": "7"
                    },
                    {
                        "cbc:Description": "Que el Software disponga de un kit de conexi\u00f3n LC-ICPMS para el control total e integrado de los sistemas de HPLC ya disponibles en el laboratorio",
                        "cbc:WeightNumeric": "9"
                    },
                    {
                        "cbc:Description": "Que se disponga de un sistema de apantallamiento de la antorcha para asegurar que todas las especies ionizadas formadas en el plasma tengan la misma energ\u00eda cin\u00e9tica.",
                        "cbc:WeightNumeric": "7"
                    },
                    {
                        "cbc:Description": "Que la celda de colisi\u00f3n/reacci\u00f3n est\u00e9 basada en un sistema octopolar.",
                        "cbc:WeightNumeric": "8"
                    },
                    {
                        "cbc:Description": "Ampliaci\u00f3n de garant\u00eda por encima de 2 a\u00f1os en los mismos t\u00e9rminos que los establecidos en el pliego de cl\u00e1usulas administrativas.",
                        "cbc:WeightNumeric": "5"
                    }
                ]
            },
