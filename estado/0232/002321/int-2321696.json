            "cac:AwardingTerms": {
                "cac:AwardingCriteria": [
                    {
                        "cbc:Description": "Compatibilidad del equipo con el programa Micromanager para el control de todas las funciones del microscopio",
                        "cbc:WeightNumeric": "10"
                    },
                    {
                        "cbc:Description": "Tiempo del cambio de filtros de fluorescencia (emisi\u00f3n y excitaci\u00f3n) menor de 30 ms",
                        "cbc:WeightNumeric": "10"
                    },
                    {
                        "cbc:Description": "Tiempo de enfoque del hardware de control autom\u00e1tico del foco menor de 700 ms",
