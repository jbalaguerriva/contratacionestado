                    {
                        "cbc:EvaluationCriteriaTypeCode": {
                            "@listURI": "http://contrataciondelestado.es/codice/cl/2.0/TechnicalCapabilityTypeCode-2.0.gc",
                            "#text": "OSR-TECH"
                        },
                        "cbc:Description": "El equipo t\u00e9cnico deber\u00e1 tener conocimientos y experiencia en los sectores que competen a la actividad municipal en la prestaci\u00f3n de servicios b\u00e1sicos, relacionados con instalaciones municipales"
                    },
                    {
                        "cbc:EvaluationCriteriaTypeCode": {
                            "@listURI": "http://contrataciondelestado.es/codice/cl/2.0/TechnicalCapabilityTypeCode-2.0.gc",
                            "#text": "OSR-TECH"
                        },
                        "cbc:Description": "El Jefe de Proyecto debe tener experiencia en la elaboraci\u00f3n y gesti\u00f3n de la Encuesta de Infraestructura y Equipamientos Locales"
                    },
                    {
                        "cbc:EvaluationCriteriaTypeCode": {
                            "@listURI": "http://contrataciondelestado.es/codice/cl/2.0/TechnicalCapabilityTypeCode-2.0.gc",
                            "#text": "OSR-TECH"
                        },
                        "cbc:Description": "El equipo t\u00e9cnico deber\u00e1 tener conocimientos y experiencia en uso de tablas en base datos, uso de aplicaciones windows, internet, ArcGIS, autocad"
                    },
                    {
                        "cbc:EvaluationCriteriaTypeCode": {
                            "@listURI": "http://contrataciondelestado.es/codice/cl/2.0/TechnicalCapabilityTypeCode-2.0.gc",
                            "#text": "OSR-TECH"
                        },
                        "cbc:Description": "El equipo t\u00e9cnico deber\u00e1 tener conocimientos y experiencia en trabajos de campo para toma de datos referidos a infraestructuras y equipamientos locales, conocimiento en toma de datos mediante GPS, interpretaci\u00f3n cartogr\u00e1fica"
                    }
