            "cac:AwardingTerms": {
                "cac:AwardingCriteria": [
                    {
                        "cbc:Description": "Empresas en expansi\u00f3n qu abran un nuevo centro de trabajo que lleve aparejado un incremento neto de empleo por cuenta ajena en \u00e9ste.",
                        "cbc:WeightNumeric": "4"
                    },
                    {
                        "cbc:Description": "Empresar constituidad por personas desempleadas pertenecientes a cualquiera de los siguientes colectivos: j\u00f3venes menores de 30 a\u00f1os, inmigrantes, mayores de 45 a\u00f1os, parados de larga duraci\u00f3n, personas con discapacidad, situaci\u00f3n de especial vulnerabilidad o riesgo de exclusi\u00f3n social.",
                        "cbc:WeightNumeric": "1"
                    },
                    {
                        "cbc:Description": "Formaci\u00f3n reglada relacionada con la actividad y puesto a desarrollar.",
                        "cbc:WeightNumeric": "0.75"
                    },
                    {
                        "cbc:Description": "Experiencia laboral relacionada con la actividad a desarrollar.",
                        "cbc:WeightNumeric": "0.5"
                    },
                    {
                        "cbc:Description": "Otras empresas.",
                        "cbc:WeightNumeric": "1"
                    },
                    {
                        "cbc:Description": "Empresas de nueva creaci\u00f3n , con independencia de su forma jur\u00eddica, promovidas por personas desempleadas que creen su propio empleo.",
                        "cbc:WeightNumeric": "8"
                    },
                    {
                        "cbc:Description": "Cualificaci\u00f3n/formaci\u00f3n complementaria relacionada con la actividad empresarial y puesto a desarrollar, as\u00ed como, con nuevas tecnolog\u00edas y prevenci\u00f3n de riesgos laborales.",
                        "cbc:WeightNumeric": "0.5"
                    },
                    {
                        "cbc:Description": "Empresas formalmente constituidas, con independencia de su forma jur\u00eddica, que hayan sido promovidas por personas desempleadas que hayan creado su propio puestos de trabajo y que a fecha de presentaci\u00f3n de la solicitud, lleven menos de 24 meses ejerciendo la actividad empresarial.",
                        "cbc:WeightNumeric": "6"
                    }
                ]
            },
