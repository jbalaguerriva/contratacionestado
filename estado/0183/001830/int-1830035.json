            "cbc:FundingProgram": "Programa Operativo FEDER de la Comunitat Valenciana vigente para el periodo 2014-2020. EJE PRIORITARIO 4.3.1",
            "cac:RequiredFinancialGuarantee": {
                "cbc:GuaranteeTypeCode": {
